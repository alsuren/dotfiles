{
  // "[toml]": {
  //   "editor.defaultFormatter": "vscode.json-language-features"
  // },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    ".env": "shellscript",
    "*.sql": "pgsql",
    "*.conf": "ssh_config"
  },
  "[terraform]": {},
  "editor.multiCursorModifier": "ctrlCmd",
  "terraform.indexing": {
    "enabled": false,
    "liveIndexing": false,
    "delay": 500,
    "exclude": [
      ".terraform/**/*",
      "**/.terraform/**/*"
    ]
  },
  "terraform.languageServer": {
    "external": true,
    "args": [
      "serve"
    ]
  },
  "go.formatTool": "goimports",
  "go.useLanguageServer": true,
  "git.autofetch": true,
  "terminal.integrated.allowChords": false,
  "go.lintOnSave": "off",
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.acceptSuggestionOnEnter": "off",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "rust-analyzer.updates.askBeforeDownload": false,
  "shellformat.effectLanguages": [
    "shellscript",
    "dockerfile",
    "dotenv",
    "hosts",
    "jvmoptions",
    "ignore",
    "gitignore",
    "properties",
    "spring-boot-properties",
    "azcli"
  ],
  "cSpell.userWords": [
    "actix",
    "Airbyte",
    "alsuren",
    "arrayvec",
    "azurecr",
    "bazel",
    "bflush",
    "BINTRAY",
    "bluez",
    "bwrite",
    "cascadia",
    "cloudbbq",
    "clsx",
    "codegen",
    "Collab",
    "Compat",
    "consts",
    "cowboyed",
    "CRDTs",
    "creds",
    "dapr",
    "datascience",
    "deps",
    "Deque",
    "devs",
    "displaydoc",
    "entrypoint",
    "excalidraw",
    "excalidrawlib",
    "faas",
    "filegroup",
    "fname",
    "fnhs",
    "gcloud",
    "genrule",
    "gitattributes",
    "grafana",
    "gunicorn",
    "hdrs",
    "Homebrew's",
    "hoverboard",
    "hoverkite",
    "httpserver",
    "ibazel",
    "imageops",
    "ipad",
    "iphone",
    "ipynb",
    "JDBC",
    "Kahootz",
    "Kanban",
    "keygen",
    "keyvalue",
    "krustlet",
    "kubectl",
    "kubernetes",
    "Lanczos",
    "leafnode",
    "leds",
    "libfuzzer",
    "linecap",
    "linejoin",
    "mkdir",
    "mmacosx",
    "mutex",
    "myspace",
    "myuser",
    "NATS",
    "nbstripout",
    "objc",
    "openfaas",
    "opentelemetry",
    "passwordless",
    "Peekable",
    "petgraph",
    "pgoutput",
    "pipefail",
    "plotly",
    "postgresql",
    "poweroff",
    "preemptible",
    "privs",
    "pulumi",
    "quickbuild",
    "quickinstall",
    "readline's",
    "realpath",
    "repos",
    "reqwest",
    "resave",
    "resaving",
    "REUSEPORT",
    "roapi",
    "rustc",
    "SEMATEXT",
    "shortcode",
    "skylib",
    "sqlx",
    "srcs",
    "structopt",
    "Tably",
    "TEMPDIR",
    "tempfile",
    "testid",
    "testonly",
    "tinyjson",
    "TMPDIR",
    "todos",
    "toolchain",
    "unmount",
    "unrecognised",
    "untar",
    "urql",
    "USART",
    "vercel",
    "viewports",
    "wapc",
    "wascc",
    "wasi",
    "wasm",
    "wasmcloud",
    "xcrun",
    "zipkin"
  ],
  "rust-analyzer.diagnostics.disabled": [
    "macro-error"
  ],
  "files.exclude": {
    "**/node_modules": true,
    "vendor/airbyte": true
  },
  "docker.showStartPage": false,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "python.insidersChannel": "weekly",
  "jupyter.askForKernelRestart": false,
  "jupyter.alwaysTrustNotebooks": true,
  "jupyter.codeLenses": " jupyter.debugcell, jupyter.runallcellsabove, jupyter.runcell",
  "jupyter.collapseCellInputCodeByDefault": false,
  "jupyter.maxOutputSize": 800,
  "go.toolsManagement.autoUpdate": true,
  "workbench.startupEditor": "none",
  "terminal.integrated.scrollback": 100000,
  "terminal.integrated.tabs.enabled": true,
  "notebook.lineNumbers": "on",
  "redhat.telemetry.enabled": true,
  "terminal.integrated.env.osx": {
    "SUPER_SECRET_SECRET": "stuff"
  },
  "[go]": {
    "editor.insertSpaces": false,
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": false
    },
    "editor.suggest.snippetsPreventQuickSuggestions": false
  },
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.defaultProfile.osx": "bash",
  "editor.codeLens": false,
  "evenBetterToml.formatter.arrayAutoExpand": false,
  "rust-analyzer.checkOnSave.allTargets": false,
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "rust-analyzer.inlayHints.typeHints.enable": false,
  "workbench.colorTheme": "Default Light+",
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "[toml]": {
    "editor.formatOnSave": false
  },
  "[scss]": {
    "editor.formatOnSave": false,
    "editor.suggest.insertMode": "replace"
  },
  "editor.formatOnSave": true,
  "window.zoomLevel": 1
}
